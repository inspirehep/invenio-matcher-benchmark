  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effect of noise on joint remote preparation of multi-qubit state</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-31</subfield>
