  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">An improved network model for railway traffic</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2017-01-26</subfield>
