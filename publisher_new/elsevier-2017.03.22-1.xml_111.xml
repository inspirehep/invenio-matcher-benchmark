  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Dynamic earthquake rupture simulations on nonplanar faults embedded in 3D geometrically complex, heterogeneous elastic solids</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">2016-01-15</subfield>
    <subfield code="t">published</subfield>
