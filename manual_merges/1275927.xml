  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Fermi-LAT Detection of Gravitational Lens Delayed Gamma-ray Flares from Blazar B0218+357</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Fermi-LAT Detection of Gravitational Lens Delayed Gamma-ray Flares from Blazar B0218+357</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
