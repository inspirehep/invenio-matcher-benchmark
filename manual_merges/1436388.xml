  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Effects of quantum deformation on the integer quantum Hall effect</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">$\kappa$-deformed Landau levels and implications on the integer quantum Hall effect</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Effects of quantum deformation on the integer quantum Hall effect</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
