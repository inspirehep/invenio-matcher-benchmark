  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Holographic Schwinger effect in a confining background with Gauss-Bonnet corrections</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
