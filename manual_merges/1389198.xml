  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">A viscous blast-wave model for relativistic heavy-ion collisions</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">A viscous blast-wave model for relativistic heavy-ion collisions</subfield>
  </datafield>
  <datafield tag="269" ind1=" " ind2=" ">
    <subfield code="c">2015-08-24</subfield>
  </datafield>
  <datafield tag="300" ind1=" " ind2=" ">
