  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">Study of Centrality Dependence of Kinetic Freeze-out Conditions in Pb + Pb Collisions at Root(sNN)= 2.76 TeV</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="9">arXiv</subfield>
    <subfield code="a">Centrality Dependence of Thermal Freeze-out conditions at LHC in Pb - Pb Collisions at root(sNN) = 2.76 TeV</subfield>
  </datafield>
  <datafield tag="246" ind1=" " ind2=" ">
    <subfield code="a">Study of Centrality Dependence of Kinetic Freeze-out Conditions in Pb + Pb Collisions at Root(sNN)= 2.76 TeV</subfield>
    <subfield code="9">arXiv</subfield>
  </datafield>
